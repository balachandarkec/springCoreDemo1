<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="address" class="spcoreDemo.models.Address">
 <property name="street" value="192, APJ Road"></property>
 <property name="city" value="Chennai"></property>
 <property name="pin" value="600097"></property>

</bean>

<bean id="address1" class="spcoreDemo.models.Address">
 <property name="street" value="192, Vivekananda Road"></property>
 <property name="city" value="Chennai"></property>
 <property name="pin" value="600007"></property>

</bean>


<bean id="employee" class="spcoreDemo.models.Employee">
 <property name="skills">
  <list>
   <value>java</value>
    <value>java FSD</value>
     <value>Angular</value>
      <value>React</value>
       <value>DevOps</value>
  </list>
 
 </property>

</bean>

</beans>